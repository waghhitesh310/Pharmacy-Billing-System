<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>servletClass.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servletClass.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>servletClass.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDrugs</servlet-name>
        <servlet-class>servletClass.AddDrugs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InventoryAddServlet</servlet-name>
        <servlet-class>servletClass.InventoryAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchMedicine</servlet-name>
        <servlet-class>servletClass.SearchMedicine</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateBillServlet</servlet-name>
        <servlet-class>servletClass.CreateBillServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StockReportServlet</servlet-name>
        <servlet-class>servletClass.StockReportServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InventoryAddServlet</servlet-name>
        <url-pattern>/InventoryAddServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchMedicine</servlet-name>
        <url-pattern>/SearchMedicine</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateBillServlet</servlet-name>
        <url-pattern>/CreateBillServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StockReportServlet</servlet-name>
        <url-pattern>/StockReportServlet</url-pattern>
    </servlet-mapping>
</web-app>
